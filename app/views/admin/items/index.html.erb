<h1>Admin Items</h1>
<% @items.each do |item| %>
  <p>Title: <%= item.title %></p>
  <p>Description: <%= item.description %></p>
  <p>Price: <%= item.price %></p>
  <p>Category: <%= item.category.name %></p>
  <p><%= item.image %></p>
  <p>Status: <%= item.status %></p>
  <p><%= link_to "Edit", edit_admin_item_path(item) %></p>
  <p><%= link_to "Item Page", item_path(item) %></p>
<% end %>

